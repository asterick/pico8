__lua__
-- mandelbrot zoomer
-- by zep

scale = 0.02
cam_x = 0
cam_y = 0
offset = 0
t = 0
show_inst = true

function _draw()

 y0 = 0
 if (show_inst) then
  rectfill(0,0, 127,7,0)
  print("(a) (b) : zoom   dpad : move",
    8,1, 7)
  y0 = 8
 end

	
	-- low res first
	
	for y=y0,127,4 do
		for x=0,127,4 do
			cx = (x-64) * scale + cam_x
			cy = (y-64) * scale + cam_y
			zx = 0 zy = 0
			colorcounter = 0
			while (zx*zx + zy*zy < 4 and colorcounter < 16) do
				tempx = zx*zx-zy*zy+cx
				zy=2*zx*zy+cy
				zx=tempx
				colorcounter = colorcounter + 1
			end
			pset(x, y, 16-(colorcounter%8))
		end
	end
	
	for y=y0,127,4 do
		for x=0,127,4 do		
			-- should we fill?
			
			col = pget(x, y)
			if (col ~= pget(x + 4, y) or
			    col ~= pget(x, y + 4) or
			    col ~= pget(x + 4, y + 4))
			then
			
				for y0=0,3 do
					if (y0 == 0) then x0 = 1 else x0 = 0 end
					while x0 < 4 do
				
						cx = (x-64+x0) * scale + cam_x
						cy = (y-64+y0) * scale + cam_y
	
						zx = 0 zy = 0
						colorcounter = 0
						while (zx*zx + zy*zy < 4 and colorcounter < 16) do
							tempx = zx*zx-zy*zy+cx
							zy=2*zx*zy+cy
							zx=tempx
							colorcounter = colorcounter + 1
						end
	
						pset(x+x0, y+y0, 16-(colorcounter%8))
						x0 = x0 + 1
					end
				end
			else
				-- fill
				rectfill(x, y, x+3, y+3, col)
			end
		end
	end

	
end

function _update()	
	
 if (btn(0)) then 
  cam_x = cam_x - scale * 4 end
 if (btn(1)) then
  cam_x = cam_x + scale * 4 end
 if (btn(2)) then
  cam_y = cam_y - scale * 4 end
 if (btn(3)) then
  cam_y = cam_y + scale * 4 end
	
 if (btn(4)) then 
  scale = scale * 0.9 end
 if (btn(5)) then
  scale = scale * 1.1 end

 for i=0,5 do
  if(btn(i)) then show_inst=false end
 end
	
-- t = (t + 1) % 30
 t = t + 1

	if (scale < 0.01) then
		-- to do: test error recovery
		-- gablarble()
	end	

end







